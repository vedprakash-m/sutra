      // Only fetch when auth state is determined
      fetchData();
    } else
    /* istanbul ignore next */
    {
      cov_zferir0lg().b[3][1]++;
    }
  }, [token, ...dependencies]);
  /* istanbul ignore next */
  cov_zferir0lg().s[20]++;
  return {
    ...state,
    refetch: fetchData
  };
}
function useAsyncAction() {
  /* istanbul ignore next */
  cov_zferir0lg().f[4]++;
  const [loading, setLoading] =
  /* istanbul ignore next */
  (cov_zferir0lg().s[21]++, (0, react_1.useState)(false));
  const [error, setError] =
  /* istanbul ignore next */
  (cov_zferir0lg().s[22]++, (0, react_1.useState)(null));
  const {
    token
  } =
  /* istanbul ignore next */
  (cov_zferir0lg().s[23]++, (0, AuthProvider_1.useAuth)());
  /* istanbul ignore next */
  cov_zferir0lg().s[24]++;
  const execute = async action => {
    /* istanbul ignore next */
    cov_zferir0lg().f[5]++;
    cov_zferir0lg().s[25]++;
    setLoading(true);
    /* istanbul ignore next */
    cov_zferir0lg().s[26]++;
    setError(null);
    /* istanbul ignore next */
    cov_zferir0lg().s[27]++;
    try {
      /* istanbul ignore next */
      cov_zferir0lg().s[28]++;
      // Set the auth token for API requests
      if (token) {
        /* istanbul ignore next */
        cov_zferir0lg().b[4][0]++;
        cov_zferir0lg().s[29]++;
        api_1.apiService.setToken(token);
      } else
      /* istanbul ignore next */
      {
        cov_zferir0lg().b[4][1]++;
      }
      const result =
      /* istanbul ignore next */
      (cov_zferir0lg().s[30]++, await action());
      /* istanbul ignore next */
      cov_zferir0lg().s[31]++;
      return result;
    } catch (error) {
      const errorMessage =
      /* istanbul ignore next */
      (cov_zferir0lg().s[32]++, error instanceof Error ?
      /* istanbul ignore next */
      (cov_zferir0lg().b[5][0]++, error.message) :
      /* istanbul ignore next */
      (cov_zferir0lg().b[5][1]++, "An error occurred"));
      /* istanbul ignore next */
      cov_zferir0lg().s[33]++;
      setError(errorMessage);
      /* istanbul ignore next */
      cov_zferir0lg().s[34]++;
      throw error;
    } finally {
      /* istanbul ignore next */
      cov_zferir0lg().s[35]++;
      setLoading(false);
    }
  };
  /* istanbul ignore next */
  cov_zferir0lg().s[36]++;
